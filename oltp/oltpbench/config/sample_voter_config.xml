<?xml version="1.0"?>
<parameters>

    <!-- Connection details -->
    <dbtype>mysql</dbtype>
    <driver>com.mysql.jdbc.Driver</driver>
    <DBUrl>jdbc:mysql://localhost:3306/voter</DBUrl>
    <username>root</username>
    <password></password>
    <isolation>TRANSACTION_SERIALIZABLE</isolation>
    <uploadCode></uploadCode>
    <uploadUrl></uploadUrl>
    <scalefactor>1</scalefactor>
    <loaderThreads>1</loaderThreads>
    
    <!-- The workload -->
    <terminals>3</terminals>
    <works>
        <work>
          <time>10</time>
          <rate>1</rate>
          <weights>100</weights>
        </work>
    </works>

    <!-- Voter Procedures declaration -->
    <transactiontypes>
        <transactiontype>
                <name>Vote</name>
        </transactiontype>
    </transactiontypes>
</parameters>
